class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        n = len(nums)
        k = 2
        j = 0
        for i in range(n):
            if i!= n-2 and i!=n-1 and nums[i]==nums[i+1] and nums[i]==nums[i+2]:
                continue
            elif i== n-2 or i== n-1:
                nums[j] = nums[i]
                j+=1
            else:
                k+=1
                nums[j] = nums[i]
                j+=1
        return k        